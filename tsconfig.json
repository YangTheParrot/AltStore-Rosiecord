{
  "compilerOptions": {
    "target": "es2017",                                  
    "module": "esnext",                                
    "moduleResolution": "node",                       
    "resolveJsonModule": true,                        
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true                                 
  }
}
